		"exitbutton": 		 { "up": "circlebutton.up", 	"down": "circlebutton.down", 	"imageUp": "exit.icon",				"imageDown": "exit.icon",	"pressedOffsetY": -2 }
	},
	
	"com.badlogic.gdx.scenes.scene2d.ui.Window$WindowStyle": {
		"default":	{ "background": panel, "titleFont": font }
	},
	
	"com.badlogic.gdx.scenes.scene2d.ui.CheckBox$CheckBoxStyle": {
		"default": { "checkboxOff": checkbox.off, "checkboxOn": checkbox.on, "font": font }
