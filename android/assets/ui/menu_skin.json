{
	"com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle": {
		"label":	{ "font": font }
		"levelButtonSmallFont": { "font": levelbuttonsmallfont }
		"title":	{ "font": titlefont } 
	},
	
	"com.badlogic.gdx.scenes.scene2d.ui.Button$ButtonStyle": {
		"arrowright": 	 { "up": "arrow.right.up", 	"down": "arrow.right.down"  }
		"arrowleft": 	 { "up": "arrow.left.up", 	"down": "arrow.left.down"   }
		"optionsbutton": { "up": "circlebutton.up", "down": "circlebutton.down" }
	},
	
	"com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle": {
		"levelbutton": 	{ "up": "levelbutton.up", 	"down": "levelbutton.down", "disabled": "levelbutton.disabled", "pressedOffsetY": -2, "font": levelbuttonfont }
	},
	
	"com.badlogic.gdx.scenes.scene2d.ui.ImageButton$ImageButtonStyle": {
		"playbutton": 		 { "up": "circlebutton.up", 	"down": "circlebutton.down", 	"imageUp": "play.icon",				"imageDown": "play.icon",			"pressedOffsetY": -2 }
